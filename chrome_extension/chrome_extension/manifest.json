{
    
    "name": "PaperPlus",
    
    "description" : "An Implementation of Paper Retrieval Google Extension Based on NLP",
    
    "version": "1.0",
   
    "manifest_version": 2,

    "browser_action": {
        "default_popup": "popup.html"
      },
    "commands": {
        "_execute_browser_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "MacCtrl+Shift+F"
          },
          "description": "Opens popup.html"
        }
    },
    "background": {
        
        "scripts": ["background.js"],
        "persistent": true
      }
}